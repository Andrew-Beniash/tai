# Azure Functions Configuration
FUNCTIONS_WORKER_RUNTIME=python
AzureWebJobsStorage=UseDevelopmentStorage=true

# Azure Storage for Document Generation
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=your_account_name;AccountKey=your_account_key;EndpointSuffix=core.windows.net
AZURE_STORAGE_CONTAINER_NAME=documents

# Google Drive API Configuration (for document operations)
GOOGLE_DRIVE_CREDENTIALS_FILE=path_to_google_credentials.json
GOOGLE_DRIVE_PROJECT_ROOT_FOLDER_ID=your_google_drive_folder_id
GOOGLE_DRIVE_TEMPLATES_FOLDER_ID=your_templates_folder_id

# Mock External APIs
MOCK_RISK_REVIEW_API_URL=https://example.com/risk-review
MOCK_TAX_REVIEW_SYSTEM_URL=https://example.com/tax-review

# Logging Configuration
APPINSIGHTS_INSTRUMENTATIONKEY=your_app_insights_key
